{
  "npmScope": "myorg",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "todos": {
      "tags": ["todos"]
    },
    "todos-e2e": {
      "tags": ["todos"],
      "implicitDependencies": ["todos"]
    },
    "todos-api": {
      "tags": ["todos"]
    },
    "blog": {
      "tags": ["blog"]
    },
    "blog-api": {
      "tags": ["blog"]
    },
    "api-interfaces": {
      "tags": ["todos", "blog"]
    }
  }
}
